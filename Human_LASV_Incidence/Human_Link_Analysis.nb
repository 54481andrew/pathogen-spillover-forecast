Cell[TextData[{
 "Derive relationship between ",
 StyleBox["seroprevalence",
  IgnoreSpellCheck->True],
 " and new case rate"
}], "Subchapter",ExpressionUUID->"6db9f21f-ddc9-4383-8ef3-524f635138a8"],

Cell["\<\
The remainder of this analysis is identical to what was performed above. Note \
that the equilibrium values of s are smaller when a > 0. Find the \
equilibrium. \
\>", "Text",ExpressionUUID->"961d8c79-dedd-4235-a68f-01c9da908a93"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"eq", " ", "=", " ", 
  RowBox[{"FullSimplify", "[", 
   RowBox[{"Solve", "[", 
    RowBox[{
     RowBox[{"{", 
